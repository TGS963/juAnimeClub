generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model SeasonalTop {
	id	Int @id @default(autoincrement())
	image	String @unique
	name String?
	rating String
	mal String
}
model Trending {
	id	Int @id @default(autoincrement())
	image	String @unique
	title String
	source String?
}
model BigCard {
	id	Int @id @default(autoincrement())
	image	String @unique
	title String?
	desc String?
	button String?
}